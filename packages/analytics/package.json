{
  "name": "@bfra.me/renovate-action-analytics",
  "version": "0.0.0-semantic-release",
  "private": true,
  "description": "Analytics and reporting infrastructure for Renovate action",
  "keywords": [
    "analytics",
    "renovate",
    "github-actions",
    "metrics",
    "monitoring"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bfra-me/renovate-action.git",
    "directory": "packages/analytics"
  },
  "license": "MIT",
  "author": "Marcus R. Brown <git@mrbro.dev>",
  "type": "module",
  "main": "dist/index.js",
